<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Admin</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">
  </head>
  <body>
    <div class="container text-center">
        <div class="row mt-5">
          <div class="col ">
            <div class="list-group list-group-flush">
                <a href="/admin?q=new" class="list-group-item list-group-item-action active">New Work</a>
                <a href="/admin?q=pen" class="list-group-item list-group-item-action">Pending</a>
                <a href="/admin?q=suc" class="list-group-item list-group-item-action">Success</a>
                <a href="/admin?q=adandessn" class="list-group-item list-group-item-action">Advertisement/Essential</a>
                <a href="/admin?q=test" class="list-group-item list-group-item-action">Testimonials</a>
                <a href="/" class="list-group-item list-group-item-action bg-warning">Home</a>
                <a href="/logout" class="list-group-item list-group-item-action bg-danger">Logout</a>
              </div>
          </div>
          <div class="col">
            <% if(q==="new"){ %>
              <%- include('partials/customerdata',{data: data, q:q}); %>
              <a href="/admin/ref" class="btn btn-primary">Refresh</a>
             
            <%} else if(q==="pen"){ %>
              <%- include('partials/cust_pen',{data: data, q:q}); %>
            
            <%} else if(q==="suc"){ %>
              <%- include('partials/cust_suc',{data: data, q:q}); %>
            
            <%} else if(q==="adandessn"){ %>
              <%- include('partials/pageessential',{data: data, q:q}); %>
            
            <%}else if(q==="test"){ %>
              <%- include('partials/customerfeedback',{data: data, q:q}); %>
            <% } %> 
            <%- include('partials/editform') %> 
          </div>
          
        </div>
      </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa" crossorigin="anonymous"></script>
  </body>
 
</html>