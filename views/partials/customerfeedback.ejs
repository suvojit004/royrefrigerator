<table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Name</th>
        <th scope="col">Description</th>
        
      </tr>
    </thead>
    <tbody>
      <%  data.forEach(element => { %> 
        <tr><form action="/admin/feedback" method="post">
          <th scope="row"><button type="submit" class="btn btn-primary">Edit</button></th>
          <td><textarea name="title" cols="30" rows="1"><%= element.title %></textarea> </td>
          <td><textarea name="description" cols="30" rows="1"><%= element.description %></textarea></td>
          <td class="invisible"><input type="text" name="id" id="" value=<%= element._id %>></td>
        </form>
        </tr>
      <% }); %> 
    </tbody>
  </table>